### bio-table
# first convert the file to a tab-delimited file
/usr/local/bin/bio-table table1.csv --in-format csv --format tab > test1.tab
/usr/local/bin/bio-table table2.csv --in-format csv --format tab > test2.tab


# merge two files with the same column nam/usr/local/bin/bio-table table1.csv --in-format csv --format tab > test1.tabes
bio-table --merge table1.csv table2.csv > merge.csv


